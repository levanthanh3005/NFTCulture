    --name ethganache \
    -v $HOME/Desktop/Tools/NFTDemo/blockchainAPI:/opt/APP/truffle \
    -p 3000:3000 \
    -p 8545:8545 \
    levanthanh3005/ethganache:v0.35

#For ubuntu
docker run -it --rm  \
	--name ethganache \
	-v $HOME/Desktop/ethereumdocker/ganache/Truffle:/opt/APP/truffle \
    --network bridge \
    -p 3000:3000 \
    -p 8545:8545 \
    ethganache

docker run -it --rm  \
    --name ethganache \
    -v $HOME/Desktop/ethereumdocker/ganache/Truffle:/opt/APP/truffle \
    -v /dev/snd:/dev/snd --privileged \
    -v /tmp/.X11-unix:/tmp/.X11-unix \
    -e uid=$(id -u) -e gid=$(id -g) \
    -e DISPLAY=unix$DISPLAY \
    --network bridge \
    ethganache

#start ganache
ganache-cli -m "replace purpose priority swift air maid night august math coral tag antenna" -h 0.0.0.0

ganache-cli -m "shed release sport light manage account pride carbon habit cereal behind stay" -h 0.0.0.0

#start wallet in UI
docker exec -it ethganache ./opt/APP/wallet/mist --rpc http://0.0.0.0:8545

#do with truffle
docker exec -it ethganache bash
cd /opt/APP/truffle


