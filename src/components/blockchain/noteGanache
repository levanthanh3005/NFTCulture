#For mac
open -a XQuartz
ip=$(ifconfig en0 | grep inet | awk '$1=="inet" {print $2}')
xhost + $ip

docker run -it --rm \
    -v /tmp/.X11-unix:/tmp/.X11-unix \
    --network bridge \
    --name ethganache \
    -v $HOME/Desktop/Tools/NFTDemo/blockchainAPI:/opt/APP/truffle \
    -e DISPLAY=$(ifconfig en0 | grep inet | awk '$1=="inet" {print $2}'):0 \
    -p 3000:3000 \
    -p 8545:8545 \
    levanthanh3005/ethganache:v0.35

# For no UI

